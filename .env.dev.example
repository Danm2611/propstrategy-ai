# Development Environment (Digital Ocean)
ENVIRONMENT=dev

# Database - Supabase Dev Database
DATABASE_URL=postgresql://your-user:your-pass@db.dev.supabase.co:5432/postgres

# NextAuth
NEXTAUTH_URL=https://propstrategy-dev.yourdomain.com
NEXTAUTH_SECRET=development-nextauth-secret-32-chars-min

# API Keys (shared across environments)
ANTHROPIC_API_KEY=your-anthropic-key
PERPLEXITY_API_KEY=your-perplexity-key

# Stripe Test Keys (for dev environment)
STRIPE_PUBLIC_KEY=pk_test_your-test-key
STRIPE_SECRET_KEY=sk_test_your-test-key
STRIPE_WEBHOOK_SECRET=whsec_test_your-webhook-secret

# Email
RESEND_API_KEY=your-resend-key

# Redis - Upstash Dev Instance
REDIS_URL=redis://your-dev-redis-url

# Admin settings
ADMIN_EMAILS=your-email@example.com
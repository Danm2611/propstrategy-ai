# Production Environment (Digital Ocean)
ENVIRONMENT=production

# Database - Supabase Production Database
DATABASE_URL=postgresql://your-user:your-pass@db.prod.supabase.co:5432/postgres

# NextAuth
NEXTAUTH_URL=https://app.propstrategy.ai
NEXTAUTH_SECRET=production-nextauth-secret-32-chars-min-super-secure

# API Keys
ANTHROPIC_API_KEY=your-anthropic-key
PERPLEXITY_API_KEY=your-perplexity-key

# Stripe Live Keys (for production)
STRIPE_PUBLIC_KEY=pk_live_your-live-key
STRIPE_SECRET_KEY=sk_live_your-live-key
STRIPE_WEBHOOK_SECRET=whsec_live_your-webhook-secret

# Email
RESEND_API_KEY=your-resend-key

# Redis - Upstash Production Instance
REDIS_URL=redis://your-prod-redis-url

# Admin settings
ADMIN_EMAILS=your-email@example.com